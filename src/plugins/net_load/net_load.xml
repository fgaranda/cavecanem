<plugin name="net_load">
  <dll>net_load</dll>
  <create_function>create_net_load</create_function>
  <publishing_period_sec>1</publishing_period_sec>
  <dds_properties>
    <dds_qos_library>testing</dds_qos_library>
    <dds_qos_profile>testing</dds_qos_profile>
  </dds_properties>
  <plugin_config>
  </plugin_config>

  <type_definition type_name="net_load">
    <struct name="net_load">
      <member name="hostname" type="string" stringMaxLength="50" key="true"/>
      <member name="ts" type="long"/>
      <member name="device" type="string" stringMaxLength="256" key="true"/>
      <member name="type" type="string" stringMaxLength="64"/>
      <member name="description" type="string" stringMaxLength="256"/>
      <member name="hwaddr" type="string" stringMaxLength="20"/>
      <member name="address" type="string" stringMaxLength="20"/>
      <member name="destination" type="string" stringMaxLength="20"/>
      <member name="broadcast" type="string" stringMaxLength="20"/>
      <member name="flags" type="long"/>
      <member name="mtu" type="long"/>
      <member name="metric" type="long"/>
      <member name="rx_packets" type="longLong"/>
      <member name="rx_bytes" type="longLong"/>
      <member name="rx_dropped" type="long"/>
      <member name="rx_overruns" type="long"/>
      <member name="rx_frame" type="long"/>
      <member name="tx_packets" type="longLong"/>
      <member name="tx_bytes" type="longLong"/>
      <member name="tx_errors" type="long"/>
      <member name="tx_dropped" type="long"/>
      <member name="tx_overruns" type="long"/>
      <member name="tx_collisions" type="long"/>
      <member name="tx_carrier" type="long"/>
    </struct>
  </type_definition>
</plugin>
    
